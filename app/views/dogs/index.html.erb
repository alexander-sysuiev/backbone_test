<div id="dogs_app">
  <h1>New Dog</h1>
  <%= render 'form' %>

  <h1>Listing dogs</h1>

  <table>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>

  <% @dogs.each do |dog| %>
    <tr>
      <td><%= dog.name %></td>
      <td><%= dog.age %></td>
      <td><%= link_to 'Show', dog %></td>
      <td><%= link_to 'Edit', edit_dog_path(dog) %></td>
      <td><%= link_to 'Destroy', dog, confirm: 'Are you sure?', method: :delete %></td>
    </tr>
  <% end %>
  </table>

  <br />

  <%= link_to 'New Dog', new_dog_path %>

  <h1>Listing dog via backbone</h1>
  <table id="dog_table">
    <tr>
      <th>Name</th>
      <th>Age</th>
    </tr>
  </table>

  <script id="dog_template" type="text/html">
    <td>{{name}}</td>
    <td>{{age}}</td>
  </script>
</div>